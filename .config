CONFIG_TARGET_ath79=y
CONFIG_TARGET_ath79_generic=y
CONFIG_TARGET_ath79_generic_DEVICE_mikrotik_routerboard-711-5hnd=y

# LUCI webfelület
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-ssl=y
CONFIG_PACKAGE_luci-app-opkg=y
CONFIG_PACKAGE_luci-app-firewall=y
CONFIG_PACKAGE_luci-theme-bootstrap=y

# SSH
CONFIG_DROPBEAR_SUPPORT_OPENSSH=y
CONFIG_DROPBEAR_CURVE25519=y

# UART és kernel debug
CONFIG_KERNEL_PRINTK=y
CONFIG_KERNEL_DEBUG_INFO=y
CONFIG_KERNEL_KALLSYMS=y
CONFIG_KERNEL_CRASHLOG=y

# SNMP
CONFIG_PACKAGE_net-snmp=y
CONFIG_PACKAGE_net-snmp-utils=y
CONFIG_PACKAGE_snmpd=y

# Egyéb hasznos eszközök
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_nano=y
CONFIG_PACKAGE_ip-full=y
CONFIG_PACKAGE_tcpdump=y
CONFIG_PACKAGE_kmod-usb-core=y
CONFIG_PACKAGE_kmod-usb2=y
CONFIG_PACKAGE_kmod-usb-serial=y
CONFIG_PACKAGE_kmod-usb-serial-ftdi=y
CONFIG_PACKAGE_kmod-usb-serial-pl2303=y
CONFIG_PACKAGE_kmod-usb-acm=y
